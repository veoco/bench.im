{
  "name": "bench.im",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "extract": "formatjs extract 'src/**/*.jsx' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
