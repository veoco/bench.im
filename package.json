    "compile-zh": "formatjs compile lang/zh.json --ast --out-file compiled-lang/zh.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.5",
    "react-router-dom": "^6.3.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.0.7",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "babel-plugin-formatjs": "^10.3.25",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "vite": "^3.0.0"
  }
}
